DATABASE_URL="postgresql://benefits:localpassword@localhost:5432/benefits_platform"

# Server
PORT=4000
NODE_ENV=development

# Add other env vars as needed
PHI_ACCESS_LOGGING=true
AUTO_LOGOUT_MINUTES=15
COMPLIANCE_AUDIT_ENABLED=true
COMPLIANCE_LOG_PHI=false

# ========== FEATURES ==========
FEATURE_ENROLLMENT=true
FEATURE_AI_COACH=false
FEATURE_DOCUMENT_AI=false
FEATURE_CARRIER_SUBMIT=false

# ========== NOTIFICATIONS ==========
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_FROM=noreply@benefits-platform.local
EMAIL_MOCK_MODE=true

# ========== SFTP (Carrier Integration) ==========
SFTP_USER=carrier
SFTP_PASS=carrier_pw
EDI_MOCK_MODE=true

# TEST ENCRYPTION KEYS (Added for development)
ENCRYPTION_KEY=test-32-character-encryption-key!
ENCRYPTION_SALT=test-salt-minimum-16-characters!
